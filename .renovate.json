{
  "extends": [
      "group:socketio",
      "mergeConfidence:all-badges"
    ],
    "ignorePresets": [
      "ignoreModulesAndTests"
    ],
    "packageRules": [
      {
        "groupName": "my-custom-group",
        "matchDatasources": [
          "npm"
        ]
      },
      {
        "matchConfidence": ["high", "very high"],
        "groupName": "high merge confidence"
      }
    ]
}
